{
  "env": {
    "es6": true
  },
  "globals": {
    "CSG": true,
    "CAG": true,
    "echo": true,
    "util": true,
    "include": true,
    "union": true,
    "Parts": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  }
}
